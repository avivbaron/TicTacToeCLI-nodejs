{
  "name": "tictactoe-realtime",
  "version": "1.4.0",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "start:a": "cross-env PORT=3001 node src/server/index.js",
    "start:b": "cross-env PORT=3002 node src/server/index.js",
    "docker:redis": "docker run --name ttt-redis -p 6379:6379 -d redis:7 || true",
    "play:x:a": "node src/client/play.js X 3001",
    "play:o:b": "node src/client/play.js O 3002",
    "play:x:b": "node src/client/play.js X 3002",
    "play:o:a": "node src/client/play.js O 3001",
    "lint": "eslint .",
    "format": "prettier --write .",
    "test": "jest --runInBand",
    "play:demo": "node src/client/demo.js 3001 3002"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "ioredis": "^5.4.1",
    "jsonwebtoken": "^9.0.2",
    "swagger-ui-express": "^5.0.1",
    "ws": "^8.17.1"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "eslint": "^9.8.0",
    "ioredis-mock": "^8.9.0",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "supertest": "^7.0.0"
  },
  "jest": {
    "testEnvironment": "node"
  }
}